{
	"name": "shineunited/composer-build-minify",
	"type": "composer-plugin",
	"license": "MIT",
	"require": {
		"php": ">=5.6",
		"composer-plugin-api": "^1.0 || ^2.0",
		"shineunited/composer-build": "^0.1.2",
		"matthiasmullie/minify": "^1.3"
	},
	"require-dev": {
		"composer/composer": ">=1.0",
		"phpunit/phpunit": ">=5.5.0"
	},
	"autoload": {
		"psr-4": {
			"ShineUnited\\ComposerBuildPlugin\\Minify\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"ShineUnited\\ComposerBuildPlugin\\Minify\\Tests\\": "tests/"
		}
	},
	"extra": {
		"class": "ShineUnited\\ComposerBuildPlugin\\Minify\\Plugin"
	}
}
